<Bindings>
	<Binding name="WIMENABLE" header="WIMMOD">
			WIM_Bindings_EnableWIM();
	</Binding>
	<Binding name="WIMHISTORY">
			WIM_HistoryFrame:Show();
	</Binding>
	<Binding name="WIMSHOWNEW">
			WIM_ShowNewMessages();
	</Binding>
	<Binding name="WIMTOGGLE">
			WIM_ToggleWindow_Toggle();
	</Binding>
	<Binding name="WIMSHOWALL">
			WIM_ShowAll();
	</Binding>
	<Binding name="WIMHIDEALL">
			WIM_HideAll();
	</Binding>
</Bindings>
